<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../helpproject.xsl" ?>
<topic template="Default" lasteditedby="robert" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../helpproject.xsd">
  <title translate="true">X# Preprocessor Macros</title>
  <keywords>
    <keyword translate="true">__ARRAYBASE__</keyword>
    <keyword translate="true">__DATE__</keyword>
    <keyword translate="true">__DATETIME__</keyword>
    <keyword translate="true">__DEBUG__</keyword>
    <keyword translate="true">__FILE__</keyword>
    <keyword translate="true">__LINE__</keyword>
    <keyword translate="true">__MODULE__</keyword>
    <keyword translate="true">__SRCLOC__</keyword>
    <keyword translate="true">__TIME__</keyword>
    <keyword translate="true">__UTCTIME__</keyword>
    <keyword translate="true">__VERSION__</keyword>
    <keyword translate="true">__XSHARP__</keyword>
    <keyword translate="true">Macro</keyword>
    <keyword translate="true">Preprocessor</keyword>
  </keywords>
  <body>
    <header>
      <para styleclass="Heading1"><text styleclass="Heading1" translate="true">X# Preprocessor Macros</text></para>
    </header>
    <para styleclass="Normal"><text styleclass="Normal" translate="true">The X# preprocessor supports the following macros.</text></para>
    <para styleclass="Normal"><text styleclass="Normal" translate="true">These macros are replaced with literal values by the preprocessor</text></para>
    <para styleclass="Normal"></para>
    <para styleclass="Normal"><table styleclass="Default" rowcount="13" colcount="3">
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Name</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Type</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Descripton</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">__ARRAYBASE__</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Integer</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">0 or 1, depending on the compiler option /AZ ( 0 when /AZ is TRUE)</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">__DATE__</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">String</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Current date in YYYYMMDD format</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">__DATETIME__</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">String</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Current date in default DateTime format</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">__DEBUG__</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Logical</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Defined with TRUE when a PDB file is being generated. Otherwise not defined</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">__FILE__</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">String</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Current source file name</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">__LINE__</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Integer</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Current source line number</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">__MODULE__</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">String</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Current source file name</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">__SRCLOC__</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">String</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Current source file name &amp; Line number</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">__TIME__</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">String</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Current (local) time in HH:mm:ss format</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">__UTCTIME__</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">String</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Current Universal time in HH:mm:ss format</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">__VERSION__</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">String</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Current Compiler version (for example 1.0.0.0)</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">__XSHARP__</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Logical</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Always TRUE</text></para>
        </td>
      </tr>
    </table></para>
    <para styleclass="Normal"></para>
    <para styleclass="Normal"></para>
  </body>
</topic>
