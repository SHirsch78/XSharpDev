<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../helpproject.xsl" ?>
<topic template="Default" lasteditedby="robert" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../helpproject.xsd">
  <title translate="true">Bound Expressions</title>
  <body>
    <header>
      <para styleclass="Heading1"><text styleclass="Heading1" translate="true">Bound Expressions</text></para>
    </header>
    <para styleclass="Normal"><text styleclass="Normal" translate="true">Bound expressions are expressions that access properties or members from objects or structures</text></para>
    <para styleclass="Heading2"></para>
    <para styleclass="Normal"></para>
    <para styleclass="Code Example"><table styleclass="Default" rowcount="7" colcount="5" style="border-width:0px; border-spacing:0px; border-collapse:collapse; cell-border-width:1px; border-color:#000000; border-style:solid;">
      <thead style="vertical-align:top">
        <td style="vertical-align:middle;">
          <para styleclass="Normal"><text styleclass="Normal" style="font-weight:bold;" translate="true">Expression type</text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"><text styleclass="Normal" style="font-weight:bold;" translate="true">Syntax</text></para>
        </td>
        <td style="vertical-align:middle; width:60px;">
          <para styleclass="Normal" style="text-align:center;"><text styleclass="Normal" style="font-weight:bold;" translate="true">VO</text></para>
        </td>
        <td style="vertical-align:middle; width:60px;">
          <para styleclass="Normal" style="text-align:center;"><text styleclass="Normal" style="font-weight:bold;" translate="true">VN</text></para>
        </td>
        <td style="vertical-align:middle; width:60px;">
          <para styleclass="Normal" style="text-align:center;"><text styleclass="Normal" style="font-weight:bold;" translate="true">X#</text></para>
        </td>
      </thead>
      <tr style="vertical-align:top">
        <td style="vertical-align:middle;">
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Bound Member Access</text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Code Example"><text styleclass="Code Example" style="font-weight:normal; font-style:normal; color:#000000;" translate="true">boundexpression (DOT|COLON) simpleName</text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"><text styleclass="Normal" translate="true">Y</text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="vertical-align:middle;">
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Bound Method Call</text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Code Example"><text styleclass="Code Example" style="font-weight:normal; font-style:normal; color:#000000;" translate="true">boundexpression LPAREN argumentlist? RPAREN</text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"><text styleclass="Normal" translate="true">Y</text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="vertical-align:middle;">
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Bound Array Access</text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Code Example"><text styleclass="Code Example" style="font-weight:normal; font-style:normal; color:#000000;" translate="true">boundexpression LBRKT argumentlist RBRKT</text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"><text styleclass="Normal" translate="true">Y</text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="vertical-align:middle;">
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Bound Conditional Access </text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Code Example"><text styleclass="Code Example" style="font-weight:normal; font-style:normal; color:#000000;" translate="true">expression QMARK boundexpression</text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"><text styleclass="Normal" translate="true">Y</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="vertical-align:middle;">
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Bind Member Access</text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Code Example"><text styleclass="Code Example" style="font-weight:normal; font-style:normal; color:#000000;" translate="true">(DOT|COLON) simpleName</text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"><text styleclass="Normal" translate="true">Y</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="vertical-align:middle;">
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Bind Array Access</text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Code Example"><text styleclass="Code Example" style="font-weight:normal; font-style:normal; color:#000000;" translate="true">LBRKT argumentlist RBRKT</text></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"></para>
        </td>
        <td style="vertical-align:middle;">
          <para styleclass="Normal" style="text-align:center;"><text styleclass="Normal" translate="true">Y</text></para>
        </td>
      </tr>
    </table></para>
    <para styleclass="Heading2"></para>
    <para styleclass="Heading2"></para>
  </body>
