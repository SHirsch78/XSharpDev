<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../helpproject.xsl" ?>
<topic template="Default" lasteditedby="robert" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../helpproject.xsd">
  <title translate="true">Char Literals</title>
  <keywords>
    <keyword translate="true">Char</keyword>
  </keywords>
  <body>
    <header>
      <para styleclass="Heading1"><text styleclass="Heading1" translate="true">Char Literals</text></para>
    </header>
    <para styleclass="Normal"><text styleclass="Normal" translate="true">Character literals inside X# start and end with a single quote (&apos;) and may have embedded control characters with escaped notation:</text></para>
    <para styleclass="Normal"></para>
    <para styleclass="Normal"><table styleclass="Default" rowcount="5" colcount="2">
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Literal</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Description</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">&apos;A&apos;</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Capital A</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">&apos;\t&apos;</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Tab Character</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">&apos;\u0100&apos;</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Unicode notation for </text><text styleclass="Font Style" style="font-family:Arial; font-size:13pt; font-weight:normal; font-style:normal; text-decoration:none; text-transform:none; vertical-align:baseline; color:#000000; background-color:transparent; letter-spacing:normal; letter-scaling:100%;" translate="true">Ā (A-macron)</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">&apos;\x002a&apos;</text></para>
        </td>
        <td>
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Hex notation for character 42 (asterisk *)</text></para>
        </td>
      </tr>
    </table></para>
    <para styleclass="Normal"><tab /></para>
    <para styleclass="Normal"><tab /></para>
    <para styleclass="Normal"></para>
  </body>
</topic>
